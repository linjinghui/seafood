.content {
  height: 100%;
  overflow: auto;
  background-color: #f4f5f6;
}
.content .price {
    font-size: inherit;
    color: inherit;
}
.content > view {
    margin-bottom: 10px;
    background-color: #fff;
}
.content > .header {
    padding: 10px 0;
    line-height: 32px;
    text-align: center;
}
.content > .header > .status {
      display: block;
      font-size: 18px;
}
.content > .header > .sub {
      display: block;
}
.content > .header > button {
      display: inline-block;
      margin-top: 10px;
      margin-left: 10px;
      width: 100px;
      height: 30px;
      line-height: 28px;
      font-size: inherit;
      color: #999;
      border: solid 1px #f0f0f0;
      -webkit-border-radius: 2px;
              border-radius: 2px;
      background-color: transparent;
}
.content > .list > .item {
    padding: 10px;
    height: 50px;
    line-height: 50px;
    overflow: hidden;
    border-bottom: solid 1px #f0f0f0;
}
.content > .list > .item > .left {
      position: relative;
      float: left;
      width: 50px;
      height: 100%;
}
.content > .list > .item > .left > image {
        right: inherit;
        width: 80%;
        height: 80%;
}
.content > .list > .item > .middle {
      float: left;
      width: -webkit-calc(100% - 50px - 50px);
      width: calc(100% - 50px - 50px);
      line-height: 25px;
}
.content > .list > .item > .middle .del {
        color: #999;
}
.content > .list > .item > .right {
      float: right;
      width: 50px;
      height: 100%;
      text-align: right;
}
.content > .list.total > .item {
    padding-top: 5px;
    padding-bottom: 5px;
}
.content > .list.total > .item > .right {
      width: 120px;
}
.content > .list.type-1 > .item {
    height: unset;
    line-height: unset;
    border: 0;
}
.content > .list.type-1 > .item > .left {
      width: 80px;
      height: unset;
      color: #999;
}
.content > .list.type-1 > .item > .middle {
      width: -webkit-calc(100% - 80px);
      width: calc(100% - 80px);
      line-height: unset;
}
.content > .list.type-1 > .item .copy {
      display: inline-block;
      margin-left: 10px;
      padding: 0;
      width: 40px;
      height: 24px;
      line-height: 20px;
      color: #029c45;
      background-color: transparent;
      border: solid 1px #029c45;
      font-size: 12px;
      vertical-align: middle;
}
.content > .wrap-kf {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    line-height: 22px;
    background-color: white;
    z-index: 2;
}
.content > .wrap-kf > .main {
      width: 240px;
      height: 374px;
}
.content > .wrap-kf > .main > image {
        margin-bottom: 10px;
        width: 100%;
}
.content > .wrap-kf > .main > text {
        display: block;
}

