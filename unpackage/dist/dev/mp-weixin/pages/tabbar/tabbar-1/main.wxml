<view class="content" bind:__l="__l"><view class="header _ul"><view data-event-opts="{{[['tap',[['clkNav',[0]]]]]}}" class="{{['_li',(active.index===0)?'active':'']}}" bindtap="__e">有货<view class="wrap-icon"><uni-icon class="icon tick" type="checkmarkempty" size="32" color="{{active.index===0?'#029c45':'#000'}}"></uni-icon></view></view><view data-event-opts="{{[['tap',[['clkNav',[1]]]]]}}" class="{{['_li',(active.index===1)?'active':'']}}" bindtap="__e">价格<view class="wrap-icon"><uni-icon class="icon arrowup" type="arrowup" size="16" color="{{active.index===1&&active.dire==='up'?'#029c45':'#000'}}"></uni-icon><uni-icon class="icon arrowdown" type="arrowdown" size="16" color="{{active.index===1&&active.dire==='down'?'#029c45':'#000'}}"></uni-icon></view></view><view data-event-opts="{{[['tap',[['clkNav',[2]]]]]}}" class="{{['_li',(active.index===2)?'active':'']}}" bindtap="__e">折扣<view class="wrap-icon"><uni-icon class="icon arrowup" type="arrowup" size="16" color="{{active.index===2&&active.dire==='up'?'#029c45':'#000'}}"></uni-icon><uni-icon class="icon arrowdown" type="arrowdown" size="16" color="{{active.index===2&&active.dire==='down'?'#029c45':'#000'}}"></uni-icon></view></view></view><view class="main"><scroll-view class="scroll-Y" scroll-top="{{scrollTop}}" scroll-y="true"><good-list value="{{goods}}" data-event-opts="{{[['^input',[['__set_model',['$0','goods','$event'],['']]]]]}}" bind:input="__e"></good-list></scroll-view></view><view class="footer"><image src="{{user.avatarUrl}}" data-event-opts="{{[['tap',[['clkImage',['$event']]]]]}}" bindtap="__e"></image>{{'已选 ('+car.count+')'}}<button data-event-opts="{{[['tap',[['clkConfirm',['$event']]]]]}}" bindtap="__e">去预定</button><view class="price total _p">{{car.total}}</view></view></view>